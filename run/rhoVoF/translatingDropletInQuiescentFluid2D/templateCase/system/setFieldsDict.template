FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
//    volScalarFieldValue alpha.water 0    
    volVectorFieldValue U_pre (0 0 0)
);

regions
(
    sphereToCell
    {
        radius  @!radius!@;
        origin  (@!x_centre!@ @!y_centre!@ @!z_centre!@);

        fieldValues
        (
//            volScalarFieldValue alpha.water 1
            volVectorFieldValue U_pre (@!x_velocity!@ @!y_velocity!@ @!z_velocity!@)
        );
    }
);

