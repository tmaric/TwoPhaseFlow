FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//Direct copy from Jun
//!!! I'm NOT 100% sure this script does anything, since only to sets are selected, but nothing is done with them

#include "meshingDimensions";

actions
(
    {
        name    interfaceRegionCellCorners;
        type    pointSet;
        action  new;
        source  boxToPoint;
        sourceInfo
        {
            box (#eval{$C0 - $radius} #eval{$C1 - $radius} #eval{-1*$radius}) (#eval{$C2 + $radius} #eval{$C3 + $radius} #eval{5*$radius});
        }
    }

    {
        name    interfaceRegion;
        type    cellSet;
        action  new;
        source  pointToCell;
        sourceInfo
        {
            set     interfaceRegionCellCorners;
            option  any;
        }
    }
);

// ************************************************************************* //


