FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        fluid_to_solid
        {
            type            mappedWall;
            inGroups        1 ( wall ); //I think we need to define this differently when we do only blockmesh...
            sampleMode      nearestPatchFace;
            sampleRegion    solid;
            samplePatch     solid_to_fluid;
        }
    }
}

