FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        Top	//solid_to_fluid	//(luis): Top because upper boundary of solid domain is name Top
        {
            type            mappedWall;
            inGroups        1 ( wall );
            sampleMode      nearestPatchFace;
            sampleRegion    fluid;
            samplePatch     Bottom;	//fluid_to_solid;	//(luis): Bottom becuase lower boundary of fluid domain named that way.
        }
    }
 
}

