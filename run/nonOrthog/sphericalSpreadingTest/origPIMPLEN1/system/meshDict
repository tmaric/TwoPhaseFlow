FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    location  "system";
    object    meshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFile "sphericalSpreadingDomain.fms";

maxCellSize 0.0005;

objectRefinements
{
        box
	{
                type            box;
                cellSize        4e-05;
                centre          (0.0025 0.0025 0);
                lengthX         0.003;
                lengthY         0.003;
                lengthZ         0.0074;
        }
}

